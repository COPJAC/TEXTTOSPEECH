   "source": [
    "import pyttsx3\n",
    "\n",
    "# Initialize the pyttsx3 engine\n",
    "engine = pyttsx3.init()\n",
    "\n",
    "# Get text input from user\n",
    "text = input(\"Enter the text you want to hear: \")\n",
    "\n",
    "# Set the text to be spoken\n",
    "engine.say(text)\n",
    "\n",
    "# Run the engine to speak the text\n",
    "engine.runAndWait()\n"
   ]
  }
